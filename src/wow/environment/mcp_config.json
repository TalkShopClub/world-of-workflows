{
    "mcpServers": {
        "servicenow": {
            "command": ".venv/bin/python",
            "args": [
                "-m",
                "servicenow_mcp.cli"
            ],
            "env": {
                "SERVICENOW_INSTANCE_URL": "To be replaced by script",
